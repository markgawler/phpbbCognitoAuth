{
    "name": "mrfg/cogauth",
    "type": "phpbb-extension",
    "description": "AWS Cognito Authentication extension",
    "homepage": "http://wler.co.uk",
    "version": "1.1.1-dev",
    "time": "2018-08-02",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Mark Gawler",
            "email": "markg@wler.co.uk",
            "homepage": "http://wler.co.uk",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "composer/installers": "~1.0",
        "ext-json": "*",
        "ext-curl": "*",
        "aws/aws-sdk-php": "3.*",
		"web-token/jwt-signature": ">=2.0",
        "web-token/jwt-core": ">=2.0",
        "web-token/jwt-signature-algorithm-rsa": ">=2.0"
    },
    "extra": {
        "display-name": "AWS Cognito Authentication",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0"
        }
    },
    "require-dev": {
        "phing/phing": "2.4.*"
    }
}
